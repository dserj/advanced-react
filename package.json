{
  "name": "advanced-react",
  "version": "1.0.0",
  "main": "lib/server.js",
  "repository": "https://github.com/dserj/advanced-react.git",
  "author": "serj",
  "license": "MIT",
  "scripts": {
    "start": "npm-run-all --parallel dev webpack",
    "dev": "set NODE_PATH=./lib&& nodemon --exec babel-node lib/server.js",
    "webpack": "webpack -wd",
    "test": "set NODE_PATH=./lib&& jest --watchAll",
    "test-cover": "set NODE_PATH=./lib&& jest --coverage",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files"
  },
  "babel": {
    "presets": [
      "react",
      ["env", { "targets": { "node": "current" } }],
      "stage-2"
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "babel-eslint": "8.0.2",
    "enzyme": "3.2.0",
    "enzyme-adapter-react-16": "1.1.0",
    "eslint": "4.11.0",
    "eslint-plugin-react": "7.5.1",
    "npm-run-all": "4.1.2",
    "react-test-renderer": "16.1.1"
  },
  "dependencies": {
    "axios": "0.17.1",
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.2",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "ejs": "2.5.7",
    "express": "4.16.2",
    "jest": "21.2.1",
    "lodash.debounce": "4.0.8",
    "lodash.pickby": "4.6.0",
    "pm2": "2.7.2",
    "prop-types": "15.6.0",
    "react": "16.1.1",
    "react-dom": "16.1.1",
    "styled-components": "2.2.3",
    "webpack": "3.8.1"
  }
}
